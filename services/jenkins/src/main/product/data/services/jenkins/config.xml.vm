    <jdk>
      <name>java-1.7.0</name>
      <home>${SYSTEM_JAVA7_APP}</home>
      <properties/>
    </jdk>
    <jdk>
      <name>java-1.8.0</name>
      <home>${SYSTEM_JAVA8_APP}</home>
