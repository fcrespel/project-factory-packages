<?xml version='1.0' encoding='UTF-8'?>
<hudson.plugins.sonar.SonarPublisher_-DescriptorImpl plugin="sonar@2.1">
  <installations>
    <hudson.plugins.sonar.SonarInstallation>
      <name>sonar</name>
      <disabled>@{sonarqube.disabled}</disabled>
      <serverUrl>@{product.scheme}://@{product.domain}/sonarqube/</serverUrl>
      <mojoVersion></mojoVersion>
      <databaseUrl>jdbc:mysql://@{mysql.host}:@{mysql.port}/%{SONARQUBE_DB_NAME}?useUnicode=true&amp;characterEncoding=utf8&amp;rewriteBatchedStatements=true</databaseUrl>
      <databaseDriver>com.mysql.jdbc.Driver</databaseDriver>
      <databaseLogin>%{SONARQUBE_DB_USER}</databaseLogin>
      <databasePassword>%{SONARQUBE_DB_PASSWORD}</databasePassword>
      <additionalProperties></additionalProperties>
      <triggers>
        <skipScmCause>false</skipScmCause>
        <skipUpstreamCause>false</skipUpstreamCause>
        <envVar></envVar>
      </triggers>
      <sonarLogin>@{bot.user}</sonarLogin>
      <sonarPassword>%{BOT_PASSWORD}</sonarPassword>
    </hudson.plugins.sonar.SonarInstallation>
  </installations>
</hudson.plugins.sonar.SonarPublisher_-DescriptorImpl>